#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int N, T,start, end, min;
    vector<int> a;
    int b;
    cin>>N>>T;
    while (N--){
        cin>>b;
        a.push_back(b);
}
    while (T--)
        {
        cin>>start>>end;
        for (int i = start; i <= end; i++)
            {
            if (i == start)
               { min = a[i];
        continue;}
        if ((a[i]) < min)
            min = a[i]; 
        }
        cout<<min<<'\n';
        }
 return 0;
    }
   
